<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="font-src">
            <values>
                <value id="font1" type="host">fonts.gstatic.com</value>
                <value id="font2" type="host">fonts.googleapis.com</value>
                <value id="font3" type="host">data:</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="style1" type="host">fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="script1" type="host">www.google.com/recaptcha/</value>
                <value id="scritp2" type="host">www.gstatic.com/recaptcha/</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
